{
  "name": "@tailor-cms/ce-html-default-edit",
  "type": "module",
  "version": "0.0.1",
  "description": "Tailor CMS default HTML editor authoring component",
  "author": "Studion <info@gostudion.com> (https://github.com/tailor-cms)",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint --ext .js,.ts,.vue ./src",
    "lint:fix": "pnpm lint --fix"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@tailor-cms/eslint-config": "0.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@tailor-cms/ce-html-default-manifest": "workspace:*",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "dependencies": {
    "@tiptap/pm": "^2.2.3",
    "@tiptap/extension-color": "^2.2.3",
    "@tiptap/extension-placeholder": "^2.2.3",
    "@tiptap/extension-subscript":"^2.2.3",
    "@tiptap/extension-superscript": "^2.2.3",
    "@tiptap/extension-text-align": "^2.2.3",
    "@tiptap/extension-text-style": "^2.2.3",
    "@tiptap/extension-underline": "^2.2.3",
    "@tiptap/starter-kit": "^2.2.3",
    "@tiptap/vue-3": "^2.2.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
